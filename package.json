{
  "name": "mythoria_admin",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack --port 3001",
    "build": "next build",
    "start": "next start --port 3001",
    "lint": "next lint",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "tsx -r dotenv/config ./src/db/migrate.ts",
    "db:push": "drizzle-kit push",
    "db:reset": "tsx -r dotenv/config ./src/db/reset.ts",
    "db:setup": "npm run db:reset && npm run db:push",
    "db:studio": "drizzle-kit studio",
    "db:seed": "tsx -r dotenv/config ./src/db/seed.ts",
    "deploy": "gcloud beta builds submit --config cloudbuild.yaml"
  },
  "sideEffects": false,
  "dependencies": {
    "@auth/drizzle-adapter": "^1.10.0",
    "@floating-ui/react": "^0.27.12",
    "@google-cloud/pubsub": "^5.1.0",
    "@google-cloud/storage": "^7.16.0",
    "@google-cloud/vertexai": "^1.10.0",
    "@google/genai": "^1.7.0",
    "@tiptap/extension-color": "^2.23.0",
    "@tiptap/extension-image": "^2.23.0",
    "@tiptap/extension-link": "^2.23.0",
    "@tiptap/extension-text-align": "^2.23.0",
    "@tiptap/extension-text-style": "^2.23.0",
    "@tiptap/pm": "^2.23.0",
    "@tiptap/react": "^2.23.0",
    "@tiptap/starter-kit": "^2.23.0",
    "@types/jsdom": "^21.1.7",
    "@types/pg": "^8.15.4",
    "@types/uuid": "^10.0.0",
    "daisyui": "^5.0.43",
    "dotenv": "^16.6.0",
    "drizzle-kit": "^0.31.4",
    "drizzle-orm": "^0.44.2",
    "jsdom": "^26.1.0",
    "next": "^15.3.4",
    "next-auth": "^5.0.0-beta.29",
    "pg": "^8.16.2",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-icons": "^5.5.0",
    "react-type-animation": "^3.2.0",
    "svix": "^1.68.0",
    "tsx": "^4.20.3",
    "uuid": "^11.1.0",
    "zod": "^3.25.67"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@types/jest": "^30.0.0",
    "@types/node": "^24",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "eslint": "^9",
    "eslint-config-next": "^15.3.4",
    "jest": "^30.0.3",
    "jest-environment-jsdom": "^30.0.2",
    "tailwindcss": "^4",
    "typescript": "^5"
  }
}
