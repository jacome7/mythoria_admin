{
  "name": "mythoria_admin",
  "version": "0.3.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "next dev --turbopack --port 3001",
    "build": "cross-env NEXT_PRIVATE_SKIP_TURBOPACK=1 next build --webpack",
    "start": "next start --port 3001",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "typecheck": "tsc --noEmit",
    "format": "prettier --check .",
    "format:fix": "prettier --write .",
    "logs": "powershell -NoProfile -ExecutionPolicy Bypass -File .\\scripts\\logs.ps1",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "tsx -r dotenv/config ./src/db/migrate.ts",
    "db:push": "drizzle-kit push",
    "db:reset": "tsx -r dotenv/config ./src/db/reset.ts",
    "db:setup": "npm run db:reset && npm run db:push",
    "db:studio": "drizzle-kit studio",
    "db:seed": "tsx -r dotenv/config ./src/db/seed.ts",
    "sync-workflows-schema": "tsx -r dotenv/config ./scripts/sync-workflows-schema.ts",
    "sync-mythoria-db-schema": "tsx -r dotenv/config ./scripts/sync-mythoria-db-schema.ts",
    "deploy": "powershell -NoProfile -ExecutionPolicy Bypass -File .\\scripts\\deploy.ps1",
    "deploy:fast": "powershell -NoProfile -ExecutionPolicy Bypass -File .\\scripts\\deploy.ps1 -Fast",
    "check:env": "tsx ./scripts/check-env-parity.ts"
  },
  "sideEffects": false,
  "dependencies": {
    "@google-cloud/pubsub": "^5.2.2",
    "@google-cloud/workflows": "^5.0.0",
    "@types/jsdom": "^27.0.0",
    "@types/pg": "^8.16.0",
    "daisyui": "^5.5.14",
    "dotenv": "^17.2.3",
    "drizzle-kit": "^0.31.8",
    "drizzle-orm": "^0.45.1",
    "googleapis": "^170.1.0",
    "handlebars": "^4.7.8",
    "jsdom": "^27.4.0",
    "next": "^16.1.3",
    "next-auth": "^5.0.0-beta.30",
    "pg": "^8.17.1",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "react-icons": "^5.5.0",
    "recharts": "^3.6.0",
    "rehype-autolink-headings": "^7.1.0",
    "rehype-pretty-code": "^0.14.1",
    "rehype-slug": "^6.0.0",
    "rehype-stringify": "^10.0.1",
    "remark-gfm": "^4.0.1",
    "remark-parse": "^11.0.0",
    "remark-rehype": "^11.1.2",
    "shiki": "^3.21.0",
    "tsx": "^4.21.0",
    "unified": "^11.0.5",
    "unist-util-visit": "^5.0.0",
    "uuid": "^13.0.0",
    "vfile": "^6.0.3",
    "zod": "^4.3.5"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@next/eslint-plugin-next": "^16.1.3",
    "@tailwindcss/postcss": "^4.1.18",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.1",
    "@types/jest": "^30.0.0",
    "@types/js-yaml": "^4.0.9",
    "@types/node": "^25.0.9",
    "@types/react": "^19.2.8",
    "@types/react-dom": "^19.2.3",
    "baseline-browser-mapping": "^2.9.4",
    "cross-env": "^10.1.0",
    "eslint": "^9.39.2",
    "eslint-config-next": "^16.1.3",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-react-hooks": "^7.0.1",
    "jest": "^30.2.0",
    "jest-environment-jsdom": "^30.2.0",
    "prettier": "^3.8.0",
    "tailwindcss": "^4.1.18",
    "ts-jest": "^29.4.6",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.53.0"
  },
  "engines": {
    "node": ">=22.0.0"
  },
  "overrides": {
    "esbuild": ">=0.24.3"
  }
}
